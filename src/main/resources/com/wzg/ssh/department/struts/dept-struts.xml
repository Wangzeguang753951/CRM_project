<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">
<struts>
    <package name="com.wzg.ssh.department.struts" extends="json-default">


        <action name="saveDept" class="deptAction" method="saveOrUpdateDept">
            <result name="success" type="redirectAction">
                <param name="actionName">getDept</param>
            </result>
        </action>


        <!--点击部门管理走这个action-->
        <action name="getDept" class="deptAction" method="getDept">
            <result name="success">/pages/department/listDepartment.jsp</result>
        </action>

        <!--点击部门编辑-->
        <action name="editDept" class="deptAction">
            <result name="success">/pages/department/addOrEditDepartment.jsp</result>
        </action>

    </package>
</struts>